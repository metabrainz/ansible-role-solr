#jinja2: trim_blocks: True, lstrip_blocks: True
# {{ ansible_managed }}
{% if _solr_environment %}

{%   for key, value in (_solr_environment | dictsort) %}
{%     if value is boolean %}
{{ key }}={{ value | lower }}
{%     else %}
{{ key }}={{ value | quote }}
{%     endif %}
{%   endfor %}
{% endif %}
